<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FlowPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" ui:field="dockPanel" spacing="5">
			<g:Dock direction="NORTH">
				<g:HorizontalPanel verticalAlignment="ALIGN_BOTTOM" width="100%" height="54px" borderWidth="0">
					<g:Image url="images/logo.png" width=""/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
						<g:Grid height="" width="100%" borderWidth="0">
						<g:row>
							<g:customCell>
								<g:Label width="" horizontalAlignment="ALIGN_LEFT" ui:field="txtWelcome"/>
							</g:customCell>
							<g:customCell>
								<g:Hyperlink targetHistoryToken="newHistoryToken" width="" ui:field="cmdLogout"/>
							</g:customCell>
							<g:customCell>
								<g:Label ui:field="lblCurrentMonth"/>
							</g:customCell>
							<g:customCell>
								<g:Button text="Cacher/ Afficher le menu" ui:field="cmdMenuToggle" width=""/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Image url="images/13.gif" ui:field="imgProgressBar" width=""/>
							</g:customCell>
							<g:customCell>
								<g:Label text="." styleName="hiddenText"/>
							</g:customCell>
						</g:row>
					</g:Grid>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="WEST">
				<g:VerticalPanel ui:field="leftPanel">
					<g:Tree animationEnabled="true" styleName="sideBar" height="" width="250px" ui:field="treeMenuAdmin">
					<g:Label text="." width="" horizontalAlignment="ALIGN_LEFT" styleName="left_header"/>
					<g:Button ui:field="cmdHomepage" width="220px" height="">ACCUEIL</g:Button>
					<g:TreeItem text="ADMINISTRATION" state="true">
						<g:TreeItem text="Bulletins" state="true">
							<g:Button width="180px" text="Gestion des bulletins"></g:Button>
							<g:Button text="Creation des bulletins" width="180px" ui:field="cmdCreateBulletins"/>
						</g:TreeItem>
						<g:TreeItem text="Profils">
							<g:Button text="Profils des études" width="180px" ui:field="cmdProfileManagement"/>
						</g:TreeItem></g:TreeItem>
					<g:TreeItem text="INFORMATIONS DE BASE">
						<g:TreeItem text="Elèves">
							<g:Button text="Liste des élèves" width="180px" ui:field="cmdStudents"/>
							<g:Button text="Ajouter un élève" width="180px" ui:field="cmdCreateStudent"/>
						</g:TreeItem>
						<g:TreeItem text="Professeurs">
							<g:Button text="Liste des professeurs" ui:field="cmdProfs" width="180px"/>
							<g:Button text="Ajouter un professeur" ui:field="cmdProfsAdd" width="180px"/>
						</g:TreeItem>
						<g:TreeItem text="Branches">
						<g:Button text="Liste des branches" width="180px" ui:field="cmdBrancheList"/>
						<g:Button text="Ajouter une branche" width="180px" ui:field="cmdBrancheAdd"/>
					</g:TreeItem>
						<g:TreeItem text="Matières">
							<g:Button text="Liste des matières" ui:field="cmdSubjectList" width="180px"/>
							<g:Button ui:field="cmdSubjectAdd" width="180px" text="Ajouter un matière">Ajouter un type</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Classes">
							<g:Button ui:field="cmdClasseList" text="Liste des classes" width="180px">Liste de cours</g:Button>
							<g:Button ui:field="cmdClasseAdd" text="Ajouter un nom de classe" width="180px">Ajouter un cours</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Programmes">
							<g:Button ui:field="cmdProgramList" width="180px">Liste des programmes</g:Button>
							<g:Button ui:field="cmdProgramAdd" width="180px">Ajouter un programme</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Ecoles">
						<g:Button ui:field="cmdEcolePage" text="Liste des écoles" width="180px">Liste d'écoles</g:Button>
						<g:Button text="Ajouter un nom d&apos;école" ui:field="cmdEcoleAdd" width="180px"/>
						</g:TreeItem>
						<g:TreeItem text="Gestion d&apos;accès">
							<g:Button text="Gestion des utilisateurs" ui:field="cmdUserMgt" width="180px"/>
							<g:Button ui:field="cmdSettings" text="Bloquer l&apos;accès" width="180px"/>
						</g:TreeItem>
					</g:TreeItem>
					<g:Button text="Changer le MDP" ui:field="cmdPassword" width="220px"/>
					<g:Button ui:field="cmdContact" width="220PX">Contact</g:Button>
				</g:Tree>
					<g:Tree ui:field="treeMenuProf" styleName="sideBar">
						<g:Label styleName="left_header" text="." width=""/>
						<g:TreeItem text="ESPACE PROFESSEUR" state="true">
							<g:Button text="Saisir des notes" width="200px" ui:field="cmdMarkInput">Mettre des heures</g:Button>
							<g:Button text="Changer le MDP" width="200px" ui:field="cmdPasswordProf"/>
							<g:Button width="200px">Contact</g:Button>
						</g:TreeItem>
					</g:Tree>
					<g:Tree styleName="sideBar" ui:field="treeMenuEleve">
						<g:Label styleName="left_header" text="." width=""/>
						<g:TreeItem state="true" text="ESPACE ELEVE">
							<g:Button text="Bulletins" width="200px" ui:field="cmdBulletinViewDetail">Mettre des heures</g:Button>
							<g:Button text="Changer le MDP" width="200px" ui:field="cmdPasswordStudent"/>
							<g:Button width="200px">Contact</g:Button>
						</g:TreeItem>
					</g:Tree>
				</g:VerticalPanel>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_LEFT" height="">
					<g:FlowPanel ui:field="mainContentPanel" width="100%" height="" styleName="paneMain"/>
					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" width="100%" horizontalAlignment="ALIGN_RIGHT" spacing="5">
						<g:Label text="Copyright © Ecole Lémania" horizontalAlignment="ALIGN_LEFT"/>
						<g:Image url="images/appengine-silver-120x30.gif"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:FlowPanel>
</ui:UiBinder>
